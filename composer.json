{
    "name": "bigpaulie/fractal-laravel-service",
    "description": "Fractal service provider for Laravel Framework",
    "keywords": ["laravel", "fractal", "service", "provider"],
    "type": "library",
    "require": {
        "php": ">=5.6.4|^7",
        "illuminate/support": "^5.0",
        "league/fractal": "^0.17"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Paul P",
            "email": "bigpaulie25ro@yahoo.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "bigpaulie\\fractal\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "bigpaulie\\fractal\\FractalServiceProvider"
            ]
        }
    }
}
